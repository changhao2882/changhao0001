/* component/w-tab-control/w-tab-control.wxss */
.tab-control{
  height: 88rpx;
  line-height: 88rpx;
  background-color: rgb(196, 126, 126);
  display: flex;
}
.tab-item{
  flex: 1;
  text-align: center;
}
.active{
  color: rgb(0, 255, 13);
}
.active text{
  padding: 16rpx 10rpx;
  border-bottom: 6rpx solid red;
}